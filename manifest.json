{
  "manifest_version": 3,
  "name": "Markdown Tabs",
  "version": "1.0",
  "description": "Save all tabs in current window as a Markdown list.",
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}